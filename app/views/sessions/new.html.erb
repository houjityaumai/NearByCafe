
<div class="form">
<p class="title">ログイン</p>
<%= form_for(:session, url: login_path) do |f| %>
  <div class="input-place">
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>
  <div class="input-place">
    <%= f.label :password %>
    <%= f.password_field :password, class: "form-control" %>
  </div>
  <div class="center">
    <%= f.submit "Log in", class: "submit-btn btn btn-light btn-outline-secondary" %>
  </div>
<% end %>
<div class="center">
<%= link_to "新規登録", new_user_path, class: "new-btn btn btn-light btn-outline-secondary" %>
<div>
</div>
